{
  "name": "@joshuahelmle/mom-core-package",
  "version": "1.0.0",
  "description": "",
  "types": "./lib/cjs/types/index.d.ts",
  "files": [
    "./lib/**/*",
    "./assets/**/*"
  ],
  "main": "./lib/cjs/index.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "updateGD": "node ./lib/cjs/loadGameData.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types.index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@nimaimalle/go-time": "^1.3.3",
    "loot-table-advanced": "^1.4.6",
    "lzutf8": "^0.6.3",
    "path": "^0.12.7",
    "run-func": "^3.0.0"
  }
}
